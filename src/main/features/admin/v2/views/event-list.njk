{% extends "layout.njk" %}

{% set title = t('events | Fees Register Admin Console V2') %}
{% set heading = t('events') %}

{% block content %}

  <div class="column-full">
    <table>
      <thead>
      <tr>
        <th scope="col">Name</th>
      </tr>
      </thead>
      <tbody>
      {% for event in events %}
        <tr>
           <td>{{ event.name }}</td>
        </tr>
      {% endfor %}
      </tbody>
    </table>

    <p><a href="/admin/welcome/" class="link-back">{{ t("Back to welcome page") }}</a></p>
  </div>

{% endblock %}
