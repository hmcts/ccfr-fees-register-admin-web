{% extends "layout.njk" %}
  {% from "includes/macros/form.njk" import nextButton%}

  {% set title = t('Fees | ' + serviceName) %}
  {% block content %}
<ul class="govuk-tabs__list govuk-!-margin-top-7">
<li class="govuk-tabs__list-item ">
    <a class="govuk-tabs__tab" href="./all-fees">
        {{ t("Live fees") }}
    </a>
</li>
<li class="govuk-tabs__list-item">
    <a class="govuk-tabs__tab" href="./all-approvedbutnotlive" style="text-decoration: underline;">
      {{ t("Approved but not live fees") }}
    </a>
</li>
<li class="govuk-tabs__list-item govuk-tabs__list-item--selected">
    <a class="govuk-tabs__tab govuk-tabs__tab--selected" href="./all-discontinued" style="text-decoration: underline;">
         {{ t("Discontinued fees") }}
    </a>
</li>
</ul>
<div class="title-with-secondary">
  <h1 class="govuk-heading-xl govuk-!-margin-bottom-6 govuk-!-margin-top-7">{{t("Discontinued fees")}}</h1>
</div>
<div class="fee-register-table-size">

<div class="custom-table-body">
  <table>
    <thead>
    <tr>
    <th class="govuk-table__header table-colsl">Code</th>
      <th class="govuk-table__header table-colxl">Description</th>
      <th class="govuk-table__header table-colsl">Amount</th>
      <th class="govuk-table__header table-colml">Statutory Instrument</th>
      <th class="govuk-table__header table-colsl">SI Ref ID</th>
      <th class="govuk-table__header table-colxl">Fee Order Name</th>
      <th class="govuk-table__header table-colsl">Service</th>
      <th class="govuk-table__header table-colsl">Jurisdiction1</th>
      <th class="govuk-table__header table-colsl">Jurisdiction2</th>
      <th class="govuk-table__header table-colsl">Event</th>
      <th class="govuk-table__header table-colsl">Range from</th>
      <th class="govuk-table__header table-colsl">Range to</th>
      <th class="govuk-table__header table-colsl">Unit</th>
      <th class="govuk-table__header table-colml">Fee type</th>
      <th class="govuk-table__header table-colml">Amount type</th>
      <th class="govuk-table__header table-colsl">%</th>
      <th class="govuk-table__header table-colsl">Channel</th>
      <th class="govuk-table__header table-colsl">Keyword</th>
      <th class="govuk-table__header table-colsl">Applicant type</th>
      <th class="govuk-table__header table-colsl">Version</th>
      <th class="govuk-table__header table-colxl">Direction</th>
      <th class="govuk-table__header table-colxl">Valid from</th>
      <th class="govuk-table__header table-colxl">Valid to</th>
      <th class="govuk-table__header table-colsl">Status</th>
      <th class="govuk-table__header table-colxl">Memo</th>
      <th class="govuk-table__header table-colsl">Natural Account Code</th>
    </tr>
    </thead>
    <tbody>
    {% for fee in fees %}
    {% if fee.current_version.status === 'approved' %}
    <tr>
      <td class="font-xxsmall">
        <a href="/admin/v2/fees/{{fee.code}}">{{ fee.code }}</a>
      </td>
      <td class="font-xxsmall">{{ fee.current_version.description }}</td>
      {% if fee.current_version.flat_amount %}
      <td class="font-xxsmall">£{{ fee.current_version.flat_amount.amount }}</td>
      {% elif fee.current_version.volume_amount %}
      <td class="font-xxsmall">£{{ fee.current_version.volume_amount.amount }}</td>
      {% else %}
      <td class="font-xxsmall">{{ fee.current_version.percentage_amount.percentage }}%</td>
      {% endif %}
      <td class="font-xxsmall">{{ fee.current_version.statutory_instrument }}</td>
      <td class="font-xxsmall">{{ fee.current_version.si_ref_id }}</td>
      <td class="font-xxsmall">{{ fee.current_version.fee_order_name }}</td>
      <td class="font-xxsmall">{{ fee.service_type.name }}</td>
      <td class="font-xxsmall">{{ fee.jurisdiction1.name }}</td>
      <td class="font-xxsmall">{{ fee.jurisdiction2.name }}</td>
      <td class="font-xxsmall">{{ fee.event_type.name }}</td>
      {% if fee.min_range %}
      <td class="font-xxsmall">{{ fee.min_range }}</td>
      {% else %}
      <td/>
      {% endif %}
      {% if fee.max_range %}
      <td class="font-xxsmall">{{ fee.max_range }}</td>
      {% else %}
      <td/>
      {% endif %}
      <td class="font-xxsmall">{{ fee.range_unit }}</td>
      <td class="font-xxsmall">{{ fee.fee_type }}</td>
      {% if version.flat_amount %}
      <td >Flat</td>
      <td/>
      {% elif version.volume_amount %}
      <td >Volume</td>
      <td/>
      {% else %}
      <td/>
      <td >Percentage</td>
      {% endif %}
      <td class="font-xxsmall">{{ fee.channel_type.name }}</td>
      <td class="font-xxsmall">{{ fee.keyword }}</td>
      <td class="font-xxsmall">{{ fee.applicant_type.name }}</td>
      <td class="font-xxsmall">{{ fee.current_version.version }}</td>
      <td class="font-xxsmall">{{ fee.current_version.direction }}</td>
       {% if fee.current_version.valid_from %}
      <td class="font-xxsmall">{{ fee.current_version.valid_from | date }}</td>
      {% else %}
      <td/>
      {% endif %}
      {% if fee.current_version.valid_to %}
      <td class="font-xxsmall">{{ fee.current_version.valid_to | date }}</td>
      {% else %}
      <td/>
      {% endif %}
      <td class="font-xxsmall">{{ fee.current_version.status }}</td>
      <td class="font-xxsmall">{{ fee.current_version.memo_line }}</td>
      <td class="font-xxsmall">{{ fee.current_version.natural_account_code }}</td>
    </tr>
    {% endif %}
    {% endfor %}
    </tbody>
  </table>
</div>
</div>
  {% endblock %}
