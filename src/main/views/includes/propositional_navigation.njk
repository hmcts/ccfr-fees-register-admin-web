    <nav id="proposition-menu">
      <span id="proposition-name">{% if serviceName %} {{ t(serviceName) }} {% endif %}</span>
      <ul id="proposition-links">
        {% if isLoggedIn %}
        <li><a href="/logout">{{ t('Sign out') }} ({{ user.allInfo.forename }} {{ user.allInfo.surname }}) </a></li>
        {% endif %}
