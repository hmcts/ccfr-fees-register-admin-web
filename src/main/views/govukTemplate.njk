{% block top_of_page %}{% endblock %}
  <!DOCTYPE html>
  <!--[if lt IE 9]>
  <html class="lte-ie8" lang="{{ html_lang|default('en') }}"><![endif]-->
  <!--[if gt IE 8]><!-->
<html lang="{{ html_lang|default('en') }}"><!--<![endif]-->
<head>
  <meta charset="utf-8"/>
  <title>{{ title }}</title>

  <!--[if gt IE 8]><!-->
  <link href="{{ asset_paths['style_vendor'] }}/govuk-template.css?{{ govuk_template_version }}" media="screen"
        rel="stylesheet"/><!--<![endif]-->
  <!--[if IE 6]>
  <link href="{{ asset_paths['style_vendor'] }}/govuk-template-ie6.css?{{ govuk_template_version }}" media="screen"
        rel="stylesheet"/><![endif]-->
  <!--[if IE 7]>
  <link href="{{ asset_paths['style_vendor'] }}/govuk-template-ie7.css?{{ govuk_template_version }}" media="screen"
        rel="stylesheet"/><![endif]-->
  <!--[if IE 8]>
  <link href="{{ asset_paths['style_vendor'] }}/govuk-template-ie8.css?{{ govuk_template_version }}" media="screen"
        rel="stylesheet"/><![endif]-->
  <link href="{{ asset_paths['style_vendor'] }}/govuk-template-print.css?{{ govuk_template_version }}" media="print"
        rel="stylesheet"/>

  <!--[if IE 8]>
  <link href="{{ asset_paths['style_vendor'] }}/fonts-ie8.css?{{ govuk_template_version }}" media="all"
        rel="stylesheet"/><![endif]-->
  <!--[if gte IE 9]><!-->
  <link href="{{ asset_paths['style_vendor'] }}/fonts.css?{{ govuk_template_version }}" media="all"
        rel="stylesheet"/><!--<![endif]-->

  <link rel="mask-icon" href="{{ asset_paths['images_vendor'] }}/gov.uk_logotype_crown.svg?{{ govuk_template_version }}"
        color="#0b0c0c">
  <link rel="apple-touch-icon-precomposed" sizes="152x152"
        href="{{ asset_paths['images_vendor'] }}/images/apple-touch-icon-152x152.png?{{ govuk_template_version }}">
  <link rel="apple-touch-icon-precomposed" sizes="120x120"
        href="{{ asset_paths['images_vendor'] }}/images/apple-touch-icon-120x120.png?{{ govuk_template_version }}">
  <link rel="apple-touch-icon-precomposed" sizes="76x76"
        href="{{ asset_paths['images_vendor'] }}/images/apple-touch-icon-76x76.png?{{ govuk_template_version }}">
  <link rel="apple-touch-icon-precomposed"
        href="{{ asset_paths['images_vendor'] }}/images/apple-touch-icon-60x60.png?{{ govuk_template_version }}">

  {% if development %}
  <link href="{{ asset_paths['style_vendor'] }}/HTMLCSAuditor.css" rel="stylesheet"/>
  {% endif %}

  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta property="og:image"
        content="{{ asset_paths['images_vendor'] }}images/opengraph-image.png{{ govuk_template_version }}">

  {% block head %}{% endblock %}
</head>
  <body class="{% block body_classes %}{% endblock %} {{ 'fees-overflow-scroll' if (currentPage == 'all-fees' or currentPage == 'all-approvedbutnotlive' or currentPage == 'all-discontinued') else '' }} ">


{% block body_start %}{% endblock %}

<div id="skiplink-container">
  <div>
    <a href="#content" class="skiplink">{{ skip_link_message|default('Skip to main content') }}</a>
  </div>
</div>

{% block cookie_banner %}{% endblock %}

<header role="banner" id="global-header" class="{% block header_class %}{% endblock %}">
  <div class="header">
    <div class="header-global">
      <div class="header-proposition">
        <a href="{{ homepage_url|default('https://www.gov.uk') }}" class="custom-header-title">{{ t('Fee register') }} </a>
      <ul class="custom-header-ul">
        <a href="/admin/V2/all-fees" class="custom-draft-all-fees">{{ t('Fees') }} </a>
        {% if isEditor(user.allInfo.roles) or isAdmin(user.allInfo.roles) %}
        <a href="/admin/V2/draft-fees" class="custom-draft-all-fees">{{ t('Your Drafts') }} </a>
        <a href="/admin/v2/upload" class="custom-draft-all-fees">{{ t('Upload fees') }} </a>
        {% elif isApprover(user.allInfo.roles)%}
        <a href="/admin/V2/pending-approval" class="custom-draft-all-fees">{{ t('Approvals') }} </a>
        {% endif %}
      </ul>
      </div>
      {% block inside_header %}{% endblock %}
    </div>
    {% block proposition_header %}{% endblock %}
  </div>
</header>


{% block after_header %}{% endblock %}

<main id="content" role="main">
  {% if form %}
  {{ errorSummary(form) }}
  {% endif %}

  {% if heading %}
  <h1 class="{%if xLargeTable %}{{ xLargeTable }}{{ t('-') }}{% endif %}heading-xlarge">
    {{ heading }}
    {% if headingSecondary %}
    <span class="heading-secondary">{{ headingSecondary }}</span>
    {% endif %}
  </h1>
  {% endif %}

  {% block content %}{% endblock %}
</main>

<footer class="group js-footer" id="footer" role="contentinfo">

  <div class="footer-wrapper">
    {% block footer_top %}{% endblock %}

    <div class="footer-meta">
      <div class="footer-meta-inner">
        {% block footer_support_links %}{% endblock %}

        <div class="open-government-licence">
          <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/"
                             rel="license">Open Government Licence</a></p>

          {% block licence_message %}<p>{{ t('All content is available under the <a href="%s" rel="license">Open
          Government Licence v3.0</a>, except where otherwise stated', { postProcess: 'sprintf', sprintf:
          ['https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/'] }) | safe }}</p>{% endblock %}

        </div>
      </div>

      <div class="copyright">
        <a
          href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">{{
          crown_copyright_message|default('&copy; ' + t('Crown copyright'))|safe }}</a>
      </div>
    </div>
  </div>
</footer>

<div id="global-app-error" class="app-error hidden"></div>

<script src="{{ asset_paths['js_vendor'] }}/govuk-template.js?{{ govuk_template_version }}"></script>

{% block body_end %}{% endblock %}

</body>
</html>
