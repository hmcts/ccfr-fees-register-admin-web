{
  "name": "fees-register-admin-web",
  "version": "2.0.1",
  "private": true,
  "engines": {
    "node": ">=v18.16.1"
  },
  "scripts": {
    "start": "node server.js",
    "start:angular-dev": "ng serve --proxy-config proxy.conf.json",
    "start:express-dev": "cross-env NODE_ENV=development nodemon --inspect start.js",
    "start:express-prod": "cross-env IGNORE_CERTS=true nodemon --inspect start.js",
    "debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/ts-node --inspect --no-lazy -r tsconfig-paths/register src/main/server.ts",
    "setup": "gulp sass copy-files",
    "lint": "tslint --project tsconfig.json --project && sass-lint -v -q",
    "test": "FEES_CLIENT_ID=feeid FEES_CLIENT_SECRET=feesecret DISABLE_AUTH=true mocha --timeout 10000 --require ts-node/register -r tsconfig-paths/register $(find src/test \\( -name '*.ts' -or -name '*.js' \\) ! -path '*a11y*' ! -path '*/routes/disabled/*') --reporter mochawesome --outputDir=./output --reporter-options reportFilename=unit,inlineAssets=true,reportTitle=fees-register-admin-web-unit",
    "test:coverage": "FEES_CLIENT_ID=feeid FEES_CLIENT_SECRET=feesecret DISABLE_AUTH=true LOG_LEVEL=OFF nyc mocha --config=.mocharc.js $(find src/test \\( -name '*.ts' \\) ! -path '*a11y*')",
    "test:coverage-unit": "FEES_CLIENT_ID=feeid FEES_CLIENT_SECRET=feesecret DISABLE_AUTH=true NODE_ENV=mocha LOG_LEVEL=OFF nyc mocha --config=.mocharc.js $(find src/test \\( -name '*.ts' \\) ! -path '*a11y*' ! -path '*/routes/*')",
    "test:routes": "FEES_CLIENT_ID=feeid FEES_CLIENT_SECRET=feesecret DISABLE_AUTH=true mocha --config=.mocharc.js $(find src/test \\( -name '*.ts' -or -name '*.js' \\) -path '*/routes/*') --reporter-options reportFilename=routes,inlineAssets=true,reportTitle=fees-register-admin-web-routes",
    "test:a11y_old": "FEES_CLIENT_ID=feeid FEES_CLIENT_SECRET=feesecret DISABLE_AUTH=true mocha --compilers ts:ts-node/register -r tsconfig-paths/register src/test/a11y --timeout 60000 --reporter mochawesome --reporter-options reportFilename=a11y,inlineAssets=true,reportTitle=fees-register-admin-web-a11y",
    "test:a11y": "cross-env node scripts/pa11y",
    "test:codecov-upload": "codecov -f ./coverage-report/lcov.info",
    "fortifyScan": "./src/test/java/gradlew -p src/test/java fortifyScan",
    "sonar-scan": "sonar-scanner -Dsonar.projectKey='fees-register-admin-web' -Dsonar.projectName='Fees-Register-Admin-Web' -Dsonar.sources=src/main -Dsonar.tests=src/test -Dsonar.exclusions=src/main/public/**,src/main/server.ts,src/main/modules/**,src/main/app/idam/security.js -Dsonar.typescript.lcov.reportPaths=coverage-report/lcov.info",
    "dependency-setup-with-yarn-lock": "yarn --mutex network install --frozen-lockfile",
    "test:smoke": "npm-run-all -scl test:smoketest test:smoke:report",
    "test:smoketest": "codeceptjs run --config smoke-test/smoke-codecept.conf.js",
    "test:smoke:report": "allure generate smoke-output/reports -c -o smoke-output/allure",
    "test:functional": "npm-run-all -scl test:acceptance test:functional:report",
    "test:fullfunctional": "npm-run-all -scl test:acceptance test:functional:report",
    "test:acceptance": "codeceptjs run-workers 6 parallel --config acceptance-tests/codecept.conf.js --grep @functional",
    "test:functional:report": "allure generate functional-output/functional/reports -c -o functional-output/functional/allure",
    "test:crossbrowser": "npm-run-all -scl test:crossbrowser:e2e test:crossbrowser:report",
    "test:crossbrowser:e2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-multiple --all --config acceptance-tests/saucelabs.conf.js --grep @crossbrowser",
    "test:crossbrowser:report": "allure generate functional-output/cross-browser/reports/* -c -o functional-output/cross-browser/allure"
  },
